/* layout */
html,
body {
  overflow-x: hidden;
  -webkit-text-size-adjust: none;
  background-color: #f1f1f1;
}
body {
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-backface-visibility: hidden;
          backface-visibility: hidden;
}
@media screen and (min-width: 800px) {
  html {
    font-size: 38px !important;
  }
  body {
    overflow: hidden;
  }
  body:after {
    position: absolute;
    top: 50%;
    right: 50%;
    bottom: auto;
    left: auto;
    width: 432px;
    height: 882px;
    -webkit-transform: translate3d(50%, -50%, 0);
            transform: translate3d(50%, -50%, 0);
    content: '';
    display: block;
    background-size: contain;
  }
  body #page {
    position: absolute;
    top: 50%;
    right: 0;
    bottom: 0;
    left: 0;
    width: auto;
    height: 100%;
    -webkit-transform: translate3d(0, -50%, 0);
            transform: translate3d(0, -50%, 0);
    width: 375px;
    height: 667px;
    margin: 0 auto;
    z-index: 200;
    overflow-y: auto;
  }
}
