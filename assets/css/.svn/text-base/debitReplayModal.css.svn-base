body{margin:0;background-color:#fff;font-family:Microsoft Yahei UI,Microsoft Yahei,SimSun,sans-serif}*{-webkit-tap-highlight-color:rgba(0,0,0,0);-webkit-touch-callout:none;box-sizing:border-box}.modal-title{height:100px;-webkit-box-align:center;-ms-flex-align:center;align-items:center;font-size:15px;padding-left:48px;text-align:left;line-height:28px;vertical-align:middle}.modal-title,list{display:-webkit-box;display:-ms-flexbox;display:flex;background-color:#fff;color:#666;border-bottom:1px solid #ddd}list{padding:0 32px;cursor:pointer}list>.columns{min-height:38px;height:46px;width:100%;-webkit-box-pack:justify;-ms-flex-pack:justify;justify-content:space-between;font-size:16px;color:#666;padding-top:12px;padding-bottom:12px}list .time{margin-right:28px}