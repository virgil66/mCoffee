.main-card{background-color:#fff;border-radius:5px}.card-header{text-align:center;border-bottom:1px dashed #e5e5e5;position:relative}.card-header span.card-title{line-height:1.6rem;vertical-align:middle;font-size:16px;color:#666}.card-header span.label-icon{background-size:cover;background-position:50%;background-repeat:no-repeat;width:2.66666667rem;height:.69333333rem;display:inline-block;line-height:.69333333rem;vertical-align:middle;position:absolute;left:-2px;top:50%;margin-top:-.34666667rem;font-size:12px;color:#fff;background-image:url(../images/label_rectangle.png)}.card-header span.label-icon .wrap{width:2.4rem;height:.69333333rem;display:inline-block;float:left}.card-header span.label-icon.warning-icon{background-image:url(../images/label_rectangle_warning.png)}.over-due-title{color:#fa3b3b!important}.is-over-due{display:none!important}.over-due-hide{display:none}.handleLoanContract,.handleRepaySchedule{color:#137bd7!important;text-decoration:underline}.card-body{padding:.42666667rem .4rem;font-size:14px}.card-body .label{color:#666;width:3.2rem;max-width:120px}.card-body .summary{color:#444;-webkit-box-flex:1;-ms-flex:1;flex:1}.card-body .columns{margin-bottom:.37333333rem}.card-body .columns:last-child{margin-bottom:0}.loan-record-border{border-bottom:1px dashed #e5e5e5}.loan-record .column.label.is-one-thirds,.loan-record .column.summary.is-two-thirds{color:#999}.loan-record .debr-evidence{display:inline-block;margin:0 auto;color:#999;font-size:16px}