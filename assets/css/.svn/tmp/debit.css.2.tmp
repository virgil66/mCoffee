.box-radius {
  position: relative;
  width: 100%;
  height: 0.32rem;
  margin-top: 3px;
  margin-bottom: 3px;
}
.box-radius:before {
  width: 0.32rem;
  height: 0.32rem;
  background-color: #f1f1f1;
  content: "";
  display: inline-block;
  border-radius: 50%;
  margin-left: -0.16rem;
}
.box-radius:after {
  float: right;
  width: 0.32rem;
  height: 0.32rem;
  background-color: #f1f1f1;
  content: "";
  display: inline-block;
  border-radius: 50%;
  margin-right: -0.16rem;
}
.box-radius .box-radius-line {
  position: absolute;
  top: 50%;
  width: 100%;
  height: 1px;
  border-bottom: 1px dashed #f1f1f1;
  margin-top: 1px;
}
